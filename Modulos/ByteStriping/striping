#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5612bb9492f0 .scope module, "TBStriping" "TBStriping" 2 3;
 .timescale -9 -12;
v0x5612bb96b8c0_0 .net "TL0", 7 0, v0x5612bb96b1d0_0;  1 drivers
v0x5612bb96b9a0_0 .net "TL1", 7 0, v0x5612bb96b2b0_0;  1 drivers
v0x5612bb96ba70_0 .net "TL2", 7 0, v0x5612bb96b370_0;  1 drivers
v0x5612bb96bb70_0 .net "TL3", 7 0, v0x5612bb96b450_0;  1 drivers
v0x5612bb96bc40_0 .var "clk", 0 0;
v0x5612bb96bd30_0 .var "fromMux", 7 0;
S_0x5612bb949470 .scope module, "byteStp" "striping" 2 12, 3 3 0, S_0x5612bb9492f0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 8 "fromMux"
    .port_info 2 /OUTPUT 8 "TL0"
    .port_info 3 /OUTPUT 8 "TL1"
    .port_info 4 /OUTPUT 8 "TL2"
    .port_info 5 /OUTPUT 8 "TL3"
P_0x5612bb949640 .param/l "COM" 0 3 22, C4<10111100>;
P_0x5612bb949680 .param/l "EDB" 0 3 23, C4<11111110>;
P_0x5612bb9496c0 .param/l "END" 0 3 23, C4<11111101>;
P_0x5612bb949700 .param/l "FTS" 0 3 23, C4<00111100>;
P_0x5612bb949740 .param/l "IDL" 0 3 23, C4<01111100>;
P_0x5612bb949780 .param/l "PAD" 0 3 22, C4<11110111>;
P_0x5612bb9497c0 .param/l "SDP" 0 3 22, C4<01011100>;
P_0x5612bb949800 .param/l "SKP" 0 3 22, C4<00011100>;
P_0x5612bb949840 .param/l "STP" 0 3 22, C4<11111011>;
v0x5612bb941f80_0 .var "D", 0 0;
v0x5612bb96b1d0_0 .var "TL0", 7 0;
v0x5612bb96b2b0_0 .var "TL1", 7 0;
v0x5612bb96b370_0 .var "TL2", 7 0;
v0x5612bb96b450_0 .var "TL3", 7 0;
v0x5612bb96b580_0 .var "c", 1 0;
v0x5612bb96b660_0 .net "clk", 0 0, v0x5612bb96bc40_0;  1 drivers
v0x5612bb96b720_0 .net "fromMux", 7 0, v0x5612bb96bd30_0;  1 drivers
E_0x5612bb938ec0 .event posedge, v0x5612bb96b660_0;
    .scope S_0x5612bb949470;
T_0 ;
    %wait E_0x5612bb938ec0;
    %load/vec4 v0x5612bb941f80_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %cmpi/e 253, 0, 8;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b450_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5612bb941f80_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5612bb96b580_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b1d0_0, 0, 8;
    %load/vec4 v0x5612bb96b580_0;
    %addi 1, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x5612bb96b580_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_0.6, 4;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b2b0_0, 0, 8;
    %load/vec4 v0x5612bb96b580_0;
    %addi 1, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
    %jmp T_0.7;
T_0.6 ;
    %load/vec4 v0x5612bb96b580_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_0.8, 4;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b370_0, 0, 8;
    %load/vec4 v0x5612bb96b580_0;
    %addi 1, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
    %jmp T_0.9;
T_0.8 ;
    %load/vec4 v0x5612bb96b580_0;
    %cmpi/e 3, 0, 2;
    %jmp/0xz  T_0.10, 4;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b450_0, 0, 8;
    %load/vec4 v0x5612bb96b580_0;
    %addi 1, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
T_0.10 ;
T_0.9 ;
T_0.7 ;
T_0.5 ;
T_0.3 ;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5612bb96b720_0;
    %cmpi/e 251, 0, 8;
    %jmp/0xz  T_0.12, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5612bb941f80_0, 0, 1;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b1d0_0, 0, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
T_0.12 ;
    %load/vec4 v0x5612bb96b720_0;
    %cmpi/e 92, 0, 8;
    %jmp/0xz  T_0.14, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5612bb941f80_0, 0, 1;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b1d0_0, 0, 8;
    %load/vec4 v0x5612bb96b580_0;
    %addi 1, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
T_0.14 ;
    %load/vec4 v0x5612bb96b720_0;
    %cmpi/e 124, 0, 8;
    %jmp/0xz  T_0.16, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b1d0_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b2b0_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b370_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b450_0, 0, 8;
T_0.16 ;
    %load/vec4 v0x5612bb96b720_0;
    %cmpi/e 188, 0, 8;
    %jmp/0xz  T_0.18, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b1d0_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b2b0_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b370_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b450_0, 0, 8;
T_0.18 ;
    %load/vec4 v0x5612bb96b720_0;
    %cmpi/e 28, 0, 8;
    %jmp/0xz  T_0.20, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5612bb96b580_0, 0, 2;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b1d0_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b2b0_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b370_0, 0, 8;
    %load/vec4 v0x5612bb96b720_0;
    %store/vec4 v0x5612bb96b450_0, 0, 8;
T_0.20 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5612bb9492f0;
T_1 ;
    %delay 1000, 0;
    %load/vec4 v0x5612bb96bc40_0;
    %nor/r;
    %store/vec4 v0x5612bb96bc40_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5612bb9492f0;
T_2 ;
    %vpi_call 2 17 "$display", "------------------------------test striping-------------------------------" {0 0 0};
    %vpi_call 2 18 "$dumpvars" {0 0 0};
    %vpi_call 2 19 "$display", "time\011    clk,      fromMux,    TL0   ,    TL1   ,   TL2   ,   TL3   " {0 0 0};
    %vpi_call 2 20 "$monitor", "%g\011      %b          %h         %h        %h         %h        %h", $time, v0x5612bb96bc40_0, v0x5612bb96bd30_0, v0x5612bb96b8c0_0, v0x5612bb96b9a0_0, v0x5612bb96ba70_0, v0x5612bb96bb70_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5612bb96bc40_0, 0;
    %delay 4000, 0;
    %vpi_call 2 25 "$display", "------------------------------Agregamos SKIPS (SKP = 1C) -------------------------------" {0 0 0};
    %pushi/vec4 28, 0, 8;
    %store/vec4 v0x5612bb96bd30_0, 0, 8;
    %delay 6000, 0;
    %vpi_call 2 28 "$display", "------------------------------Agregamos IDLE (IDL = 7C) -------------------------------" {0 0 0};
    %pushi/vec4 124, 0, 8;
    %store/vec4 v0x5612bb96bd30_0, 0, 8;
    %delay 6000, 0;
    %vpi_call 2 32 "$display", "------------------------------Agregamos START (STP = FB) -------------------------------" {0 0 0};
    %pushi/vec4 251, 0, 8;
    %store/vec4 v0x5612bb96bd30_0, 0, 8;
    %delay 2000, 0;
    %vpi_call 2 36 "$display", "------------Agregamos 6 bytes seguidos de DATOS (en este caso FF simbolizan datos) ------------" {0 0 0};
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x5612bb96bd30_0, 0, 8;
    %delay 12000, 0;
    %vpi_call 2 39 "$display", "------------------------------Agregamos END (END = ) -------------------------------" {0 0 0};
    %pushi/vec4 253, 0, 8;
    %store/vec4 v0x5612bb96bd30_0, 0, 8;
    %delay 2000, 0;
    %vpi_call 2 42 "$display", "Podemos ver como inicialmente se detecta un START, se manda al LANE0, y se siguen recibiendo datos hasta que se recibe el END" {0 0 0};
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
