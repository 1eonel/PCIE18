    .scope S_0x558559445c20;
T_1 ;
    %delay 1000, 0;
    %load/vec4 v0x558559466d90_0;
    %nor/r;
    %store/vec4 v0x558559466d90_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x558559445c20;
T_2 ;
    %vpi_call 2 20 "$dumpvars" {0 0 0};
    %vpi_call 2 21 "$display", "time\011    clk,    TL0   ,    TL1   ,   TL2   ,   TL3  , tomux " {0 0 0};
    %vpi_call 2 22 "$monitor", "%g\011      %b       %h         %h        %h       %h       %h", $time, v0x558559466d90_0, v0x558559466a10_0, v0x558559466af0_0, v0x558559466bc0_0, v0x558559466cc0_0, v0x558559466e80_0 {0 0 0};
