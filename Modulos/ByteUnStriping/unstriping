    %vpi_call 2 27 "$display", "DATOS(ff)" {0 0 0};
    %vpi_call 2 28 "$display", "END(fd)" {0 0 0};
    %vpi_call 2 29 "$display", "-------------------" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x558559466d90_0, 0;
    %pushi/vec4 251, 0, 8;
    %store/vec4 v0x558559466a10_0, 0, 8;
    %delay 2000, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x558559466af0_0, 0, 8;
    %delay 2000, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x558559466bc0_0, 0, 8;
    %delay 2000, 0;
    %pushi/vec4 253, 0, 8;
    %store/vec4 v0x558559466cc0_0, 0, 8;
    %delay 2000, 0;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_2;
