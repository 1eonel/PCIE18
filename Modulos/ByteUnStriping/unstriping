v0x558559466870_0 .var "toDemux", 7 0;
E_0x5585594382c0 .event posedge, v0x5585594667b0_0;
    .scope S_0x558559445ff0;
T_0 ;
    %wait E_0x5585594382c0;
    %load/vec4 v0x5585594465a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5585594665a0_0;
    %cmpi/e 253, 0, 8;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x5585594665a0_0;
    %store/vec4 v0x558559466870_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5585594465a0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5585594666d0_0, 0, 2;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5585594666d0_0;
    %cmpi/e 0, 0, 2;
